{
  "mcp": {
    "servers": {

      "Youtube subtitles extractor": {
        "type": "sse",
        "url": "http://localhost:5295/sse"
      },

      "Yahoo Finance": {
        "command": "uvx",
        "args": [ "yfmcp@latest" ]
      },

      "Hacker News": {
        "command": "uvx",
        "args": [ "mcp-hn" ]
      },

      "AirBnB search": {
        "command": "npx",
        "args": [
          "-y",
          "@openbnb/mcp-server-airbnb",
          "--ignore-robots-txt"
        ]
      },

      "calculator": {
        "command": "uvx",
        "args": [ "mcp-server-calculator" ]
      }
    }
  }
}